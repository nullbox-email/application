{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Security-Bearer": {
        "Authority": "https://localhost:{sts_port}",
        "Audience": "nullbox.email"
    },
    "EmailIngress": {
        "HmacSecret": ""
    },
    "Cosmos": {
        "DatabaseName": "Nullbox.FabricDB",
        "EnsureDbCreated": true
    },
    "ApplicationInsights": {
        "ConnectionString": "Insert Application Insights Connection String Here"
    },
    "CqrsSettings": {
        "LogRequestPayload": true
    },
    "AzureBlobStorage": "UseDevelopmentStorage=true",
    "KeyVault": {
        "Enabled": false,
        "Endpoint": "https://VAULT-NAME-HERE.vault.azure.net/",
        "ClientId": "",
        "Secret": "",
        "TenantId": ""
    },
    "MassTransit": {
        "RetryInterval": {
            "RetryCount": 10,
            "Interval": "00:00:05"
        }
    },
    "OpenTelemetry": {
        "ServiceName": "Nullbox.Fabric",
        "ServiceInstanceId": null
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.ApplicationInsights"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [
            "FromLogContext"
        ],
        "WriteTo": [
            {
                "Name": "ApplicationInsights",
                "Args": {
                    "connectionString": "",
                    "telemetryConverter": "Serilog.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
                }
            }
        ]
    },
    "AzureMessageBus": {
        "ConnectionString": "your connection string"
    },
    "RabbitMq": {
        "Host": "localhost",
        "VirtualHost": "/",
        "Username": "guest",
        "Password": "guest"
    },
    "CorsPolicies": {
        "Default": {
            "Origins": [
                "*"
            ],
            "Methods": [
                "*"
            ],
            "Headers": [
                "*"
            ],
            "ExposedHeaders": [
                "*"
            ]
        }
    }
}
