{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Security-Bearer": {
    "Authority": "Set-by-aspire",
    "Audience": "Set-by-aspire"
  },
  "Cosmos": {
    "AccountEndpoint": "https://localhost:8081",
    "AccountKey": "",
    "DatabaseName": "Nullbox.Auth.EntraExternalIdDB",
    "EnsureDbCreated": true
  },
  "CqrsSettings": {
    "LogRequestPayload": true
  },
  "KeyVault": {
    "Enabled": false,
    "Endpoint": "https://VAULT-NAME-HERE.vault.azure.net/",
    "ClientId": "",
    "Secret": "",
    "TenantId": ""
  },
  "MassTransit": {
    "RetryInterval": {
      "RetryCount": 10,
      "Interval": "00:00:05"
    }
  },
  "ApplicationInsights": {
    "ConnectionString": "Insert Application Insights Connection String Here"
  },
  "OpenTelemetry": {
    "ServiceName": "Nullbox.Auth.EntraExternalId",
    "ServiceInstanceId": null
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.ApplicationInsights"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "ApplicationInsights",
        "Args": {
          "connectionString": "",
          "telemetryConverter": "Serilog.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
        }
      }
    ]
  },
  "HttpClients": {
    "Nullbox.Security.Tokens.Services": {
      "Uri": "https://localhost:44391/",
      "Timeout": "00:01:00"
    }
  }
}
